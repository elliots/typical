{
  "name": "typical-sample-esm",
  "version": "1.0.0",
  "description": "Sample project using Typical with Node.js ESM loader",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "node --loader typical/esm src/index.ts",
    "dev": "node --loader typical/esm --watch src/index.ts"
  },
  "dependencies": {
    "typical": "../.."
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}