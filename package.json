{
  "name": "typical",
  "version": "0.1.0",
  "description": "Runtime safe TypeScript transformer using typia",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "typical": "dist/cli.js",
    "ttsx": "bin/ttsx"
  },
  "exports": {
    ".": "./dist/index.js",
    "./tsc-plugin": "./dist/src/tsc-plugin.js",
    "./esm": "./dist/src/esm-loader-register.js",
    "./loader": "./dist/src/esm-loader.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node --test dist/test/*.test.js",
    "postinstall": "tsc",
    "run-samples": "cd samples/esm && npm install && npm run start && cd ../ttsx && npm install && npm run start && cd ../tsc && npm install && npm run build && npm run start"
  },
  "keywords": [
    "typescript",
    "runtime",
    "validation",
    "typia",
    "transformer"
  ],
  "author": "Elliot Shepherd <elliot@jarofworms.com>",
  "license": "MIT",
  "dependencies": {
    "commander": "14.0.1",
    "glob": "11.0.3",
    "minimatch": "^10.0.0",
    "ts-patch": "^3.3.0",
    "tsx": "^4.19.2",
    "typia": "9.7.2",
    "unplugin": "2.3.10"
  },
  "devDependencies": {
    "@types/node": "22",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
