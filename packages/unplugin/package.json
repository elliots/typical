{
  "name": "@elliots/unplugin-typical",
  "version": "0.3.1",
  "description": "Unplugin for typical - runtime safe TypeScript transformer",
  "keywords": [
    "esbuild",
    "farm",
    "rolldown",
    "rollup",
    "rspack",
    "runtime",
    "typescript",
    "typical",
    "unplugin",
    "validation",
    "vite",
    "webpack"
  ],
  "homepage": "https://github.com/elliots/typical#readme",
  "bugs": {
    "url": "https://github.com/elliots/typical/issues"
  },
  "license": "MIT",
  "author": "Elliot Shepherd <elliot@jarofworms.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elliots/typical.git",
    "directory": "packages/unplugin"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.mts",
        "./*"
      ]
    }
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./esbuild": "./dist/esbuild.mjs",
    "./farm": "./dist/farm.mjs",
    "./rolldown": "./dist/rolldown.mjs",
    "./rollup": "./dist/rollup.mjs",
    "./rspack": "./dist/rspack.mjs",
    "./vite": "./dist/vite.mjs",
    "./webpack": "./dist/webpack.mjs",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@elliots/typical": "workspace:*",
    "unplugin": "^2.3.11"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsdown": "^0.18.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
